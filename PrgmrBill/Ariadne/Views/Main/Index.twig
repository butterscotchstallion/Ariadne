{% include 'Header.twig' %}



<table class="table table-striped table-bordered table-condensed">
    <thead>
        <tr>
            <th>Title</th>
            <th>Threads</th>
        </tr>
    </thead>
    <tbody>
        {% for f in forums %}
            <tr>
                <td><a href="/f/{{ f.id }}">{{ f.title }}</a></td>
                <td>{{ f.threadCount }}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

{% include 'Footer.twig' %}